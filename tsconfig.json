{
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "yarn": ["src"],
      "yarn/*": ["src/*", "*"]
    },
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true
  },
  "include": [".dumirc.ts", "src/**/*"]
}
